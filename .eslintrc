{
    "extends":"eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 6
    },
    "env": {
        "node":true,
        "browser": true,
        "es6": true
    },
    "rules": {
        "comma-dangle":"off",
        "no-unused-vars":"warn",
        "import/prefer-default-export":"on",
        "import/first":"on"

    }
}